{
  "name": "my-hockey-recruiting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:webpack": "next dev --webpack",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "dotenv -e .env.local -- npx tsx prisma/seed.ts",
    "import:facilities": "dotenv -e .env.local -- npx tsx scripts/import-facilities-from-numbers.ts",
    "update:facility-images": "dotenv -e .env.local -- npx tsx scripts/update-facility-images-from-spreadsheet.ts",
    "update:facility-images:alpha": "ALPHABETICAL_MAPPING=true dotenv -e .env.local -- npx tsx scripts/update-facility-images-from-spreadsheet.ts",
    "sync-env": "dotenv -e .env.local -- npx tsx scripts/sync-env-to-vercel.ts"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- npx tsx prisma/seed.ts"
  },
